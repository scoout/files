
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:1c.1/0000:02:00.0'
	option channel '11'
	option band '2g'
	option htmode 'HE80'
	option cell_density '0'
	option txpower '22'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'psk2'
	option key 'root1990'

config wifi-iface 'wifinet1'
	option device 'radio0'
	option mode 'sta'
	option network 'wwan'
	option ssid 'A004ZTa-8C220E'
	option encryption 'psk2'
	option key 'mifi3211'
